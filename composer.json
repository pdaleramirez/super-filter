{
    "name": "pdaleramirez/super-filter",
    "description": "A Craft CMS 3 plugin that lets you build your search page with search filters from your element fields and filter element entries by categories, tags, element relations and other fields.",
    "type": "craft-plugin",
    "version": "1.7.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "search filter"
    ],
    "support": {
        "docs": "https://github.com/pdaleramirez/super-filter/blob/master/README.md",
        "issues": "https://github.com/pdaleramirez/super-filter/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dale Ramirez",
            "homepage": "https://github.com/pdaleramirez"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
          "pdaleramirez\\superfilter\\": "src/"
        }
    },
    "extra": {
        "name": "Super Filter",
        "handle": "super-filter",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/pdaleramirez/super-filter/master/CHANGELOG.md",
        "class": "pdaleramirez\\superfilter\\SuperFilter"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G",
        "testunit": [
            "Composer\\Config::disableProcessTimeout",
            "codecept run unit"
        ]
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
